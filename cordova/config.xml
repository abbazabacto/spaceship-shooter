<?xml version='1.0' encoding='utf-8'?>
<widget id="com.transformorigin.spaceshipshooter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Spaceship shooter</name>
    <description>
        Spaceship Shooter VR
    </description>
    <author email="info@transformorigin.com" href="http://transformorigin.com">
        Bart Meinders
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="xwalkVersion" value="18+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-fullscreen" spec="~1.1.0" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~1.7.0" />
    <plugin name="ionic-plugin-keyboard" spec="~2.1.0" />
    <engine name="browser" spec="~4.1.0" />
    <engine name="android" spec="~5.1.1" />
</widget>
